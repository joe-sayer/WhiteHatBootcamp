<style>
    @import url('https://fonts.googleapis.com/css?family=Quicksand&display=swap');
    * {
        font-family: Quicksand;
    }
</style>

<h1>{{this.restaurant.name}}</h1>
{{#each this.menus}}
    <h3>{{this.title}}</h3>
    <ul>
        {{#each this.items}}
        <li>{{this.name}} - {{this.price}}</li>
        {{/each}}
    </ul>
{{/each}}